<?xml version="1.0" encoding="UTF-8"?>
<xliff xmlns="urn:oasis:names:tc:xliff:document:1.2" version="1.2">
  <file original="Module.Bitzer.Approval" product-name="Sitegeist.Bitzer.Approval" source-language="en" target-language="de" datatype="plaintext">
    <body>
      <trans-unit id="approvalAssignments.label" xml:space="preserve" approved="yes">
        <source>Approval Assignments</source>
        <target>Freigabezuordnungen</target>
      </trans-unit>
      <trans-unit id="approvalAssignments.description" xml:space="preserve" approved="yes">
        <source>Approval of content in e.g. review workspaces can be assigned to Bitzer agents.</source>
        <target>Freigabe von Inhalten, z.B. aus Review-Workspaces, kann Bitzer-Agenten zugeordnet werden.</target>
      </trans-unit>

      <trans-unit id="index.assign" xml:space="preserve" approved="yes">
        <source>Assign workspace approval</source>
        <target>Arbeitsbereich-Freigabe zuweisen</target>
      </trans-unit>
      <trans-unit id="index.assignmentLegend" xml:space="preserve" approved="yes">
        <source>Active approval assignments</source>
        <target>Aktive Freigabe-Zuweisungen</target>
      </trans-unit>
      <trans-unit id="index.workspace" xml:space="preserve" approved="yes">
        <source>Workspace</source>
        <target>Arbeitsbereich</target>
      </trans-unit>
      <trans-unit id="index.agent" xml:space="preserve" approved="yes">
        <source>Agent</source>
        <target>Agent</target>
      </trans-unit>
      <trans-unit id="index.actions" xml:space="preserve" approved="yes">
        <source>Actions</source>
        <target>Aktionen</target>
      </trans-unit>
      <trans-unit id="index.reassign" xml:space="preserve" approved="yes">
        <source>Reassign workspace approval</source>
        <target>Arbeitsbereich-Freigabe neu zuweisen</target>
      </trans-unit>
      <trans-unit id="index.removeAssignment" xml:space="preserve" approved="yes">
        <source>Remove workspace approval assignment</source>
        <target>Arbeitsbereich-Freigabezuweisung entfernen</target>
      </trans-unit>
      <trans-unit id="index.removeAssignmentWarning" xml:space="preserve" approved="yes">
        <source>Do you really want to remove this assignment?</source>
        <target>Wollen sie diese Zuweisung wirklich entfernen?</target>
      </trans-unit>
      <trans-unit id="index.removeAssignmentExplanation" xml:space="preserve" approved="yes">
        <source>This operation cannot be undone.</source>
        <target>Diese Aktion kann nicht rückgängig gemacht werden.</target>
      </trans-unit>
      <trans-unit id="index.removeAssignmentCancellation" xml:space="preserve" approved="yes">
        <source>Cancel</source>
        <target>Abbrechen</target>
      </trans-unit>
      <trans-unit id="index.removeAssignmentConfirmation" xml:space="preserve" approved="yes">
        <source>Yes, remove the assignment</source>
        <target>Ja, die Zuweisung entfernen</target>
      </trans-unit>

      <trans-unit id="assignmentForm.assign" xml:space="preserve" approved="yes">
        <source>Assign workspace approval to agent</source>
        <target>Arbeitsbereich-Freigabe Agent zuweisen</target>
      </trans-unit>
      <trans-unit id="assignmentForm.workspace" xml:space="preserve" approved="yes">
        <source>Workspace</source>
        <target>Arbeitsbereich</target>
      </trans-unit>
      <trans-unit id="assignmentForm.agent" xml:space="preserve" approved="yes">
        <source>Agent</source>
        <target>Agent</target>
      </trans-unit>
      <trans-unit id="assignmentForm.assignmentConfirmation" xml:space="preserve" approved="yes">
        <source>Assign workspace approval</source>
        <target>Arbeitsbereich-Freigabe zuweisen</target>
      </trans-unit>

      <trans-unit id="reassignmentForm.reassign" xml:space="preserve" approved="yes">
        <source>Reassign workspace approval to another agent</source>
        <target>Arbeitsbereich-Freigabe einem anderen Agenten zuweisen</target>
      </trans-unit>
      <trans-unit id="reassignmentForm.workspace" xml:space="preserve" approved="yes">
        <source>Workspace</source>
        <target>Arbeitsbereich</target>
      </trans-unit>
      <trans-unit id="reassignmentForm.agent" xml:space="preserve" approved="yes">
        <source>Agent</source>
        <target>Agent</target>
      </trans-unit>
      <trans-unit id="reassignmentForm.reassignmentConfirmation" xml:space="preserve" approved="yes">
        <source>Reassign workspace approval</source>
        <target>Arbeitsbereich-Freigabe neu zuweisen</target>
      </trans-unit>

      <trans-unit id="flashMessage.workspaceAssigned.title" xml:space="preserve" approved="yes">
        <source>Workspace assigned</source>
        <target>Arbeitsbereich zugewiesen</target>
      </trans-unit>
      <trans-unit id="flashMessage.workspaceAssigned.body" xml:space="preserve" approved="yes">
        <source>Workspace "{0}" was successfully assigned to agent "{1}"</source>
        <target>Arbeitsbereich "{0}" wurde erfolgreich Agent "{1}" zugewiesen</target>
      </trans-unit>
      <trans-unit id="flashMessage.workspaceReassigned.title" xml:space="preserve" approved="yes">
        <source>Workspace reassigned</source>
        <target>Arbeitsbereich neu zugewiesen</target>
      </trans-unit>
      <trans-unit id="flashMessage.workspaceReassigned.body" xml:space="preserve" approved="yes">
        <source>Workspace "{0}" was successfully reassigned to agent "{1}"</source>
        <target>Arbeitsbereich "{0}" wurde erfolgreich Agent "{1}" neu zugewiesen</target>
      </trans-unit>
      <trans-unit id="flashMessage.workspaceAssignmentRemoved.title" xml:space="preserve" approved="yes">
        <source>Workspace assignment removed</source>
        <target>Arbeitsbereich-Zuweisung entfernt</target>
      </trans-unit>
      <trans-unit id="flashMessage.workspaceAssignmentRemoved.body" xml:space="preserve" approved="yes">
        <source>Assignment for workspace "{0}" was successfully removed from agent "{1}"</source>
        <target>Zuweisung für Arbeitsbereich "{0}" an Agent "{1}" wurde erfolgreich entfernt</target>
      </trans-unit>
    </body>
  </file>
